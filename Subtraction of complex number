class Complex{
    int real;
    int img;
    Complex(int real,int img)
    {
        this.real=real;
        this.img=img;
    }
    public static Complex sub(Complex x,Complex y)
    {
        return new Complex(x.real-y.real,x.real-y.img);
        
    }
}
class eg1{
    public static void main(String args[])
    {
        Complex x=new Complex(3,4);
        Complex y=new Complex(2,3);
        Complex z=Complex.sub(x,y);
        System.out.println(z.real+"-"+z.img+"i");
    }
}
