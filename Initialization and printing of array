//WAP to initialize and print an array.
import java.util.*;
public class Arrex
{
	public static void main(String[] args) {
		int size;
		Scanner sc = new Scanner(System.in);
		System.out.println("Enter Size of an array : ");
		size = sc.nextInt();
		int a[] = new int[size];
		for(int i=0;i<size;i++){
		    a[i]=i;
		}
		for(int i=0;i<size;i++){
		    System.out.print(a[i]+" ");
		}
	}
}
